{
  "manifest_version": 2,
  "name": "Enspire Search",
  "version": "1.0.0",
  "description": "Simple access to Enspire search for Northern",
  "short_name": "enspire_search",
  "permissions": ["storage"], // remove all_urls?
  "content_scripts": [],
  "browser_action": {
   "default_title": "Enspire Search for Northern",
   "default_popup": "",
   "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "omnibox": {
    "keyword": "ens"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  }
}